    "repositories": [],
    "require": {
        "php": ">=7.1",
        "laravel/framework": "5.3.*",
        "intervention/image": "^2.3",
        "php-ffmpeg/php-ffmpeg": "^0.9.3",
        "interactivesolutions/honeycomb-core": "~1.0.0"
    },
    "require-dev": {
        "brianium/paratest": "^0.14",
        "mockery/mockery": "^0.9",
        "orchestra/testbench-browser-kit": "^3.3",
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "interactivesolutions\\honeycombresources\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "test": [
            "paratest -p4 --colors"
        ]
    },
    "config": {
        "secure-http": false,
        "platform": {
            "php": "7.1"
        }
    }
}